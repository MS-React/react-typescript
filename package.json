{
  "name": "react-typescript",
  "version": "0.0.1",
  "description": "react with typescript",
  "main": "src/app/index.tsx",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:analyze": "webpack-bundle-analyzer buildlog/stats.json dist -l debug",
    "lint": "tslint -c .tslint.json \"src/app/**/*.ts?(x)\"",
    "prebuild": "npm run validate",
    "prebuild:analyze": "webpack --config webpack.prod.js --profile --json > buildlog/stats.json",
    "prepush": "npm run validate",
    "prestart": "npm run validate",
    "prevalidate": "rimraf buildlog",
    "sass-lint": "sass-lint --config .sasslintrc.json \"app/**/*.scss\" --verbose --no-exit",
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=dev webpack-dev-server --open --config webpack.dev.js",
    "start:local": "cross-env NODE_ENV=local webpack-dev-server --open --config webpack.dev.js",
    "test": "cross-env NODE_ENV=test && jest --coverage",
    "test:dev": "cross-env NODE_ENV=test && jest --watch --coverage",
    "validate": "run-p -c sass-lint test"
  },
  "author": "Making Sense",
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.116",
    "@types/lodash.get": "^4.4.2",
    "@types/prop-types": "^15.5.4",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-router-redux": "^5.0.15",
    "@types/reactstrap": "^6.0.2",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.1",
    "cross-env": "^5.2.0",
    "history": "4.7.2",
    "js-logger": "^1.4.1",
    "lodash": "^4.17.10",
    "lodash.get": "^4.4.2",
    "react": "^16.4.2",
    "react-bootstrap-table-next": "^0.1.13",
    "react-bootstrap-table2-paginator": "^0.1.5",
    "react-dom": "^16.4.2",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "reactstrap": "^6.1.0",
    "redux": "3.7.2",
    "redux-actions": "^2.4.0",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "2.2.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.0",
    "@types/jest": "^23.3.1",
    "awesome-typescript-loader": "^5.2.0",
    "babel-jest": "^23.4.2",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-react": "7.7.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "^0.14.3",
    "jest": "22.4.3",
    "jest-html-reporter": "^2.4.1",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "redux-devtools-extension": "2.13.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.6.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "2.0.15",
    "webpack-dev-server": "3.1.3",
    "webpack-merge": "4.1.2"
  }
}